# This file contains the environment variables needed for the Skeptical Wombat application.
# Copy this file to .env and fill in the values.

# The Firebase configuration object, as a JSON string.
VITE_FIREBASE_CONFIG='{"apiKey":"YOUR_API_KEY","authDomain":"YOUR_AUTH_DOMAIN","projectId":"YOUR_PROJECT_ID","storageBucket":"YOUR_STORAGE_BUCKET","messagingSenderId":"YOUR_MESSAGING_SENDER_ID","appId":"YOUR_APP_ID"}'

# Your Google Gemini API Key
VITE_GEMINI_API_KEY="YOUR_GEMINI_API_KEY"

# The name of the Gemini model to use
VITE_GEMINI_MODEL_NAME="gemini-1.5-flash-preview-0514"
